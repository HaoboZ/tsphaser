{
	"include": [
		"app.ts"
	],
	"exclude": [
		"../../node_modules"
	],
	"compilerOptions": {
		"target": "es5",
		"moduleResolution": "node",
		"outDir": "../../build/server",
		"noImplicitAny": false,
		"strictNullChecks": false,
		"allowSyntheticDefaultImports": true,
		"allowJs": true,
		"inlineSourceMap": true,
		"typeRoots": [
			"../../node_modules/@types"
		],
		"types": [
			"node",
			"express",
			"socket.io",
			"socket.io-client"
		]
	}
}
