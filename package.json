{
	"name": "tsphaser",
	"version": "1.0.0",
	"engines": {
		"node": "6.11.4"
	},
	"main": "main.js",
	"scripts": {
		"build-client": "webpack --mode development",
		"build-client-dev": "webpack --mode development --watch",
		"build-server": "tsc -p src/server/tsconfig.json",
		"build-server-dev": "tsc -w -p src/server/tsconfig.json",
		"run-server": "node app.js",
		"run-server-dev": "nodemon app.js --watch build/server"
	},
	"dependencies": {
		"express": "^4.16.3",
		"jquery": "^3.3.1",
		"phaser-ce": "^2.10.1",
		"react": "^16.2.0",
		"react-dom": "^16.2.0",
		"socket.io": "^2.0.4",
		"underscore": "^1.8.3"
	},
	"devDependencies": {
		"@types/express": "^4.11.1",
		"@types/jquery": "^3.3.1",
		"@types/node": "^9.4.7",
		"@types/socket.io": "^1.4.31",
		"@types/socket.io-client": "^1.4.32",
		"babel-cli": "^6.26.0",
		"babel-loader": "^7.1.4",
		"babel-preset-env": "^1.6.1",
		"nodemon": "^1.17.1",
		"ts-loader": "^4.0.1",
		"typescript": "^2.7.2",
		"webpack-cli": "^2.0.11"
	}
}
